{
  "name": "my-ts-template",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "engines": {
    "node": ">=12"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "source": "./src/index.ts",
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/*": "eslint --fix",
    "test/*": "eslint --fix"
  },
  "keywords": [
    "rollup",
    "ts",
    "template"
  ],
  "files": [
    "dist/*",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/coverage-istanbul": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "dayjs": "^1.11.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.8.0",
    "rollup": "^3.5.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  },
  "author": "shaolianfk",
  "license": "MIT",
  "dependencies": {}
}